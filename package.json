{
  "name": "kiita-app",
  "version": "1.0.0",
  "description": "My lovely Nuxt.js project",
  "author": "ChangJoo Park",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest --watchAll"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.4.0",
    "@nuxtjs/axios": "^5.0.0",
    "body-parser": "^1.18.3",
    "date-fns": "^1.29.0",
    "express": "^4.15.3",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.2.1",
    "morgan": "^1.9.0",
    "nuxt": "^1.0.0",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.16",
    "autoprefixer": "^7.1.6",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^22.4.4",
    "cross-env": "^5.1.5",
    "eslint": "^4.15.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "faker": "^4.1.0",
    "jest": "^22.4.4",
    "jest-vue-preprocessor": "^1.4.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.4",
    "sass-loader": "^7.0.1",
    "sequelize": "^4.37.8",
    "sqlite3": "^4.0.0",
    "webpackbar": "^2.6.1"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js",
      "^~/(.*)$": "<rootDir>/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/components/**/*.{js, vue}",
      "**/server/utils/*.{js}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
